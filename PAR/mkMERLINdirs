mkdir alloutput
dos2unix *.1
mkdir input1
mv *.1 input1
cp parametric.model input1
cp runwipemerlin input1
cd input1
mv map.1 mapfile.map
mv names.1 datfile.dat
mv pedin.1 pedfile.ped
./runwipemerlin
awk '/^       POSIT/,/^Graphical/' output | sed '/^$/d' > tempoutput
cp tempoutput ../alloutput/output1
cd ..
dos2unix *.2
